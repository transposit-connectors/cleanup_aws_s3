{
  "v2": {
    "transposit": {
      "dependencies": [{
        "name": "transposit/aws_s3:v2.*.*",
        "alias": "aws_s3"
      }],
      "operations": [{
        "id": "delete_old_s3_objects",
        "function": {
          "parameters": [{
            "name": "bucketName",
            "default": {
              "text": "[enter your s3 bucket name]"
            },
            "type": "STRING"
          }],
          "js": "delete_old_s3_objects.js"
        }
      }, {
        "id": "get_objects",
        "tql": {
          "parameters": [{
            "name": "prefixName",
            "default": {
              "text": "[file or object prefix]"
            },
            "type": "STRING"
          }, {
            "name": "bucketName",
            "default": {
              "text": "[s3 bucket name]"
            },
            "type": "STRING"
          }],
          "tql": "SELECT * FROM aws_s3.list_objects\n  WHERE Bucket=@bucketName\n  AND prefix=@prefixName"
        }
      }, {
        "id": "delete_dryrun",
        "function": {
          "js": "delete_dryrun.js"
        }
      }],
      "dataConnections": [{
        "ref": "aws_s3.raw",
        "overrides": {
          "swagger": {
            "awsConfig": {
              "region": "us-west-2",
              "service": "s3"
            },
            "baseUrl": "https://s3.us-west-2.amazonaws.com",
            "executionLimits": {
            }
          }
        }
      }],
      "javascriptAPIVersion": "3",
      "source": "src"
    }
  }
}